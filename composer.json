{
    "name": "blackit/semantic-form",
    "description": "Semantic UI form helpers",
    "keywords": [
        "laravel",
        "blackit",
        "form",
        "semantic UI"
    ],
    "homepage": "https://github.com/blackit/semantic-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Subangkit Ramadiputra",
            "email": "mr.subangkit@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~5.6",
        "nesbot/carbon": "^1.36",
        "jenssegers/date": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "BlackIT\\SemanticForm\\": "src"
        },
        "files": [
            "src\\helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BlackIT\\SemanticForm\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackIT\\SemanticForm\\ServiceProvider"
            ],
            "aliases": {
                "Form": "BlackIT\\SemanticForm\\Facade",
                "SemanticForm": "BlackIT\\SemanticForm\\Facade"
            }
        }
    }
}
